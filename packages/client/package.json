{
  "name": "niseline-client",
  "version": "0.9.0",
  "description": "",
  "private": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "vite build",
    "lint:js": "eslint --cache '**/*.{js,ts}'",
    "lint:js:fix": "eslint --cache --fix '**/*.{js,ts}'",
    "typec": "tsc --noEmit",
    "codegen": "rm -rf src/generated/api && openapi2aspida -i ../../openapi.yml -o src/generated/api",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "author": "dyoshikawa",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "3.1.5"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "eslint": "8.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0"
  },
  "files": [
    "/dist"
  ],
  "main": "./dist/niseline-client.umd.js",
  "module": "./dist/niseline-client.es.js",
  "exports": {
    ".": {
      "import": "./dist/niseline-client.es.js",
      "require": "./dist/niseline-client.umd.js"
    }
  }
}
