{
  "name": "@niseline/niseliff",
  "version": "0.8.0",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "@line/liff": "2.18.1"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --format=esm --outfile=dist/index.js && tsc --emitDeclarationOnly --outDir dist",
    "lint:js": "eslint --cache '**/*.{js,ts}'",
    "lint:js:fix": "eslint --cache --fix '**/*.{js,ts}'",
    "typec": "tsc --noEmit"
  },
  "author": "dyoshikawa",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.14.11"
  },
  "keywords": [
    "line",
    "linebot",
    "line-api",
    "liff",
    "liff-sdk"
  ],
  "bugs": {
    "url": "https://github.com/classmethod/niseline/issues"
  },
  "homepage": "https://github.com/classmethod/niseline/#readme",
  "description": "NiseLine is inspired by LocalStack. Goal of this tool is to create a mock service for LINE."
}
