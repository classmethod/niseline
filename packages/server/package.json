{
  "name": "server",
  "version": "0.9.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "PORT=3001 node -r esbuild-register src/main.ts",
    "build": "esbuild src/main.ts --bundle --minify --sourcemap --platform=node --target=node16 --outfile=dist/index.js",
    "lint:js": "eslint --cache '**/*.{js,ts}'",
    "lint:js:fix": "eslint --cache --fix '**/*.{js,ts}'",
    "typec": "tsc --noEmit",
    "codegen": "rm -rf src/generated/api && openapi2aspida -i ../../openapi.yml -o src/generated/api"
  },
  "author": "dyoshikawa",
  "license": "MIT",
  "dependencies": {
    "@niseline/line-api-types": "file:packages/line-api-types",
    "aspida": "1.11.0",
    "cors": "2.8.5",
    "ejs": "3.1.6",
    "fastify": "3.25.3",
    "fastify-cors": "6.0.2",
    "fastify-formbody": "5.2.0",
    "fs-extra": "10.0.0",
    "inversify": "6.0.1",
    "lowdb": "3.0.0",
    "point-of-view": "5.0.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/ejs": "3.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.8",
    "@types/source-map-support": "0.5.6",
    "@types/uuid": "8.3.4",
    "esbuild": "0.14.11",
    "esbuild-register": "3.3.2",
    "eslint": "8.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0"
  }
}
