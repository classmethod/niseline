{
  "name": "server",
  "version": "0.9.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "PORT=3001 node -r esbuild-register src/main.ts",
    "build": "esbuild src/main.ts --bundle --minify --sourcemap --platform=node --target=node16 --outfile=dist/index.js",
    "lint:js": "eslint --cache '**/*.{js,ts}'",
    "lint:js:fix": "eslint --cache --fix '**/*.{js,ts}'",
    "typec": "tsc --noEmit",
    "codegen": "rm -rf src/generated/api && openapi2aspida -i ../../openapi.yml -o src/generated/api",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "author": "dyoshikawa",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.172.0",
    "@aws-sdk/lib-dynamodb": "3.172.0",
    "aspida": "1.11.0",
    "cors": "2.8.5",
    "express-openapi-validator": "4.13.8",
    "fs-extra": "10.0.0",
    "inversify": "6.0.1",
    "lowdb": "3.0.0",
    "point-of-view": "5.0.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/ejs": "3.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.8",
    "@types/source-map-support": "0.5.6",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "cross-fetch": "3.1.5",
    "esbuild": "0.14.11",
    "esbuild-register": "3.3.2",
    "eslint": "8.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "supertest": "6.2.4",
    "undici": "5.10.0",
    "uuid": "9.0.0"
  }
}
